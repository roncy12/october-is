{"components/common/quick-search":"<div class=\"search-container\">\r\n    <form class=\"form\" onsubmit=\"return false\" data-url=\"{{urls.search}}\" data-quick-search-form>\r\n        <fieldset class=\"form-fieldset\">\r\n            <div class=\"form-field\">\r\n                <label class=\"is-srOnly\" for=\"{{name}}\">{{lang \"search.quick_search.input_label\"}}</label>\r\n                <input class=\"form-input\"\r\n                       data-search-quick\r\n                       name=\"{{name}}\"\r\n                       id=\"{{name}}\"\r\n                       data-error-message=\"{{lang 'search.error.empty_field'}}\"\r\n                       placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\"\r\n                       autocomplete=\"off\"\r\n                >\r\n                     <div class=\"form-prefixPostfix wrap hide\">\r\n                <input class=\"form-input d-none\" id=\"search_query_adv\" name=\"search_query_adv\" value=\"{{forms.search.query}}\"   >\r\n                <input class=\"button button--primary form-prefixPostfix-button--postfix srch-button\" type=\"submit\" value=\"{{lang 'search.results.form_button_text'}}\">\r\n            </div>\r\n            </div>\r\n        </fieldset>\r\n    </form>\r\n    <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\r\n    <p role=\"status\"\r\n       aria-live=\"polite\"\r\n       class=\"aria-description--hidden\"\r\n       data-search-aria-message-predefined-text=\"{{{lang 'search.results.quick_count_live'}}}\"\r\n    ></p>\r\n    <input type=\"reset\" class=\"modal-close\" aria-label=\"{{lang 'common.close'}}\" data-drop-down-close role=\"reset\" value=\"x\">\r\n</div>\r\n"}
